\newcommand{\FormPi}{\mbox{\scriptsize$(\Pi F)$}}
\newcommand{\FormSigma}{\mbox{\scriptsize$(\Sigma F)$}}
\newcommand{\FormNeg}{\mbox{\scriptsize$(\neg F)$}}
\newcommand{\ElimPi}{\mbox{\scriptsize$(\Pi E)$}}
\newcommand{\ElimSigma}{\mbox{\scriptsize$(\Sigma E)$}}
\newcommand{\IntroPi}{\mbox{\scriptsize$(\Pi I)$}}
\newcommand{\IntroSigma}{\mbox{\scriptsize$(\Sigma I)$}}
